{
	"configurations": [
		{
			"name": "jvmApp",
			"type": "gradle",
			"workingDir": "$PROJECT_DIR$",
			"tasks": [
				"jvmRun"
			],
			"args": [
				"-DmainClass=MainKt",
				"--quiet",
				"-p",
				"$PROJECT_DIR$/composeApp/jvmApp"
			],
			"initScripts": {
				"flmapper": "ext.mapPath = { path -> null }"
			}
		},
		{
			"name": "Tender [refreshVersions]",
			"type": "gradle",
			"workingDir": "$PROJECT_DIR$",
			"tasks": [
				"refreshVersions"
			],
			"args": [
				""
			],
			"initScripts": {
				"flmapper": "ext.mapPath = { path -> path }"
			}
		},
		{
			"name": "CheckModulesTest",
			"type": "gradle",
			"workingDir": "$PROJECT_DIR$",
			"tasks": [
				":composeApp:shared:cleanTestDebugUnitTest",
				":composeApp:shared:testDebugUnitTest",
				"--tests",
				"\"CheckModulesTest\""
			],
			"args": [
				""
			],
			"initScripts": {
				"flmapper": "ext.mapPath = { path -> path }"
			}
		},
		{
			"name": "Generate resources",
			"type": "gradle",
			"workingDir": "$PROJECT_DIR$",
			"tasks": [
				"generateResourceAccessorsForCommonMain"
			],
			"args": [
				""
			],
			"initScripts": {
				"flmapper": "ext.mapPath = { path -> path }"
			}
		}
	]
}
